{
    "name": "kumatch/launcher-generator",
    "description":"A generator for kumatch/launcher.",
    "type":"library",
    "homepage": "https://github.com/kumatch/php-launcher-generator",
    "keywords":["generator", "service", "launcher", "container", "di", "dependency injection"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Yosuke Kumakura",
            "homepage": "https://github.com/kumatch"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "kumatch/launcher": "0.1",
        "symfony/dependency-injection": "~2.5",
        "symfony/config": "~2.5",
        "twig/twig": "~1.6.2",
        "nategood/commando": "~0.2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Kumatch\\Launcher\\": "src/"
        }
    }
}
